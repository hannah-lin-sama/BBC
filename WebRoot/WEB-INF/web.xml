<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>buserdelServlet</servlet-name>
  <servlet-class>com.bbs.servlet.BuserDel</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>buserdelServlet</servlet-name>
  <url-pattern>/userBean</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>postsdelServlet</servlet-name>
  <servlet-class>com.bbs.servlet.PostsDel</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>postsdelServlet</servlet-name>
  <url-pattern>/postsBean</url-pattern>
  </servlet-mapping>
</web-app>
